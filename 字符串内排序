#include <iostream>
#include <cstdio>     
#include <cstring>
#include <algorithm> 
using namespace std;

int main()
{
    char str[203];
    while(gets(str)!=NULL)
    {
        int lenth=strlen(str);
        int i,j;
        sort(str,str+lenth);
        for(i=0;i<lenth;i++)
        {
            printf("%c",str[i]);
        }
        printf("\n");
    }
    
    return 0;
}
